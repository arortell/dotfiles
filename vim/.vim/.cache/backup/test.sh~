set -o nounset                                  # Treat unset variables as an error
set -eo pipefail                               # exit if ANY command fails,errors from pipe
progname="$(basename "$0")"
echo "$progname"
function mytest {
      "$@"
       local status="$?"
       if [ "$status" -ne 0  ]; then
          echo "error with $1" >&2
        fi
       return "$status"
}
